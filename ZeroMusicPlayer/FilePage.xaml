<Page x:Class="ZeroMusicPlayer.FilePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ZeroMusicPlayer"
      mc:Ignorable="d" 
      d:DesignHeight="550" d:DesignWidth="750"
      Title="FilePage">

    <Page.Resources>
        <ResourceDictionary Source="ScrollBarDictionary.xaml"/>
    </Page.Resources>

    <Grid >
        <ScrollViewer Width="750" Height="550" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0">
            <StackPanel x:Name="SongsPanel" Width="750"/>
        </ScrollViewer>
    </Grid>
</Page>
