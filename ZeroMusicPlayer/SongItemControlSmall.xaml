<UserControl x:Class="ZeroMusicPlayer.SongItemControlSmall"
             x:Name="SongItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ZeroMusicPlayer"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="380">
    <Grid>
        <Label Content="{Binding Path=SongName, ElementName=SongItem}" Width="150" HorizontalAlignment="Left" Padding="0" VerticalContentAlignment="Center"/>
    </Grid>
</UserControl>
